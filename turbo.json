{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"]
    },
    "test": {
      "dependsOn": ["build", "^test"],
      "outputs": []
    },
    "artifact": {
      "dependsOn": ["build", "^artifact"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    }
  }
}
